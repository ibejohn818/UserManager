{
    "name": "ibejohn818/UserManager",
    "description": "UserManager plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php":">=5.4",
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "UserManager\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UserManager\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
