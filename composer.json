{
    "name": "ibejohn818/user-manager",
    "description": "UserManager plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php":">=5.4",
        "cakephp/cakephp": "~3.0",
        "google/apiclient": "2.*",
        "facebook/graph-sdk": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "UserManager\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UserManager\\Test\\": "tests",
            "UserManager\\Test\\Fixture": "tests/Fixture",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    },
    "scripts": {
        "post-install-cmd": "UserManager\\Console\\Installer::postInstall"
    }

}
